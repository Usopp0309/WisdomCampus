package com.guotop.palmschool.rest.entity;

import java.io.Serializable;
import java.util.List;
/**
 * 更新群组至互信bean
 * @author jfy
 *
 */
public class PostGroupInfo implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 3662927431808484684L;
	/**
	 * 群组名
	 */
	private String name;
	/**
	 * 群组人员对应互信用户ID列表
	 */
	private List<String> groupMembersHxUserIds;
	/**
	 * 管理员对应互信ID
	 */
	private String leadId; 

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public List<String> getGroupMembersHxUserIds() {
		return groupMembersHxUserIds;
	}

	public void setGroupMembersHxUserIds(List<String> groupMembersHxUserIds) {
		this.groupMembersHxUserIds = groupMembersHxUserIds;
	}

	public String getLeadId() {
		return leadId;
	}

	public void setLeadId(String leadId) {
		this.leadId = leadId;
	}
	
	
}
