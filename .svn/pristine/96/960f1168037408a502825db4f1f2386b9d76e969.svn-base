.big .stage_con ul li:hover{border:2px solid #8cc63e;}
.big .stage_con ul li i{position: absolute; top:5px;left:10px;display:block;width:30px;height:30px;color:#fff;background-color:#8cc63e;border-radius:15px;font-style: normal;text-align: center;line-height:30px;}
.big .stage_con ul li img{width:100px;height:150px;margin:10px auto;display:block;border:1px solid #d0d5d8}
.big .stage_con ul li p{font-size:14px;}
.archive_info{background-color:#f6fbf0;border-radius:20px!important;position: relative;overflow:hidden;width:80%;margin:0px auto;}
.archive_info .photo{display:block;width:70px;height:70px;position: absolute;top:70px; right:20px;border-radius:20px!important;}
.archive_info .header{text-align: center;background-color:#8cc63f;overflow:hidden}
.archive_info .header img{display:block;margin:10px auto;}
.archive_info h2{margin:0px;font-size:16px;text-align: center;font-weight:bold}
.archive_info p{margin:0px;padding:10px 0px;text-indent: 15px;color:#666666}
.archive_info p span{color:#333333}
.archive_info ul{display:block;margin:10px;background-color:#fff;overflow:hidden;}
.archive_info ul li{list-style-type: none;margin:10px auto;}
.archive_info ul li a{color:#666666;display:block;margin:0 10px;height:32px;text-indent:15px; line-height:32px;display:block;}
.archive_info ul li .active{background-color:#d4eef9;height:32px;line-height:32px;border-radius:16px!important;color:#29abe2;position: relative;}
.archive_info ul li .active i{display:block;width:32px;height:32px;background:url("../Template/binggo.png")no-repeat;position: absolute;top:0px;right:0px;}
.Output{margin:15px auto 0;display:block; text-align: center;width:80%;padding:10px 15px;color:#999;border:1px solid #999}
.Output:hover{color:#666}
.evaluation_header{}
.evaluation_header img{display:block;margin:0px 0px 0px 100px;;width:30%;height:30%;}
.evaluation_header P{padding:5px 0px;font-size:16px;margin:0px 0px 0px 100px;}
.evaluation_list{padding:0px;}
.evaluation_list li{list-style-type: none;line-height:50px}
.evaluation_list span{font-size:19px;font-weight:bold;}
.evaluation_list span i{font-size:22px;color:#dbdbdb;}
.evaluation_list textarea{resize:none;}
.m_text_center{text-align: center;}
.m_inline_block{display:inline-block;}
.m_color_aaa{color:#aaa;}
.m_color_999{color:#999;}
#static_copyRecipe{font-size:16px;}
.m_text_left{text-align: left;}
.m_margin_left_15{margin:0 15px 0 0}
.m_margin_top_15{margin:15px auto 0px;}
.m_clear{clear:both}
#file-list{display:block;overflow:hidden;}
.order{background-color:#fff;overflow:hidden;margin:0px;border-bottom:none;}
.order li{text-align: center;padding:0px;margin:0px!important;width:25%;float:left}
.order li.active{border-top:none!important;border-bottom: 3px solid #158ff2;color:#333}
.order li a{color:#666;}
.order li a i{margin:0 5px 0 0}
.nav-tabs > .active > a, .nav-tabs > .active > a:hover, .nav-tabs > .active > a:focus{border-right: none;border-left: none}
.menuconfig{}
.menuconfig table{border:1px solid #fff;}
.menuconfig table tr th,.menuconfig table tr td{border:1px solid #fff}
.menuconfig .gray{background-color:#ededed}
.menuconfig label{display:inline-block;}
.menuconfig .title_bg{background-color: #f4f4f4;display: inline-block;line-height: 30px;padding: 0 20px;margin:0 0 0 15px;}
.menuconfig .checkbox{margin:0px 15px;}
.menuconfig .checkbox input{margin-left:0px!important;margin-right:10px;}
.camera_live h3{text-indent: 15px;}
.camera_live h3 a{float:right;display:inline-block}
.camera_live h3 a i{display:inline-block;padding:5px;background-color:#e7e7e7;color:#4c4c4c;margin:0 15px 0 0}
.camera_live p a img{display:inline-block;border-radius:5px!important;}
table tr .img{height:25px;text-align:left;}
table tr .img img{height:25px;margin:0 10px 0 0}
.progress{height:2px;}
.progress-bar{height:2px;}
table tr th, table tr  td{min-height:25px;line-height:25px;}
table tr .message_content{text-align: inherit;}
.sms_server .server_info .num_input{padding:0px;margin:0px;border-radius:0px!important; background-color:#fff;border:1px solid #ccc;color:#666;cursor:text;width:50px;display:inline-block;}
@media (max-width: 1025px) {
	.archive_info .photo{right: 10px;top:60px;width:60px;height:60px;}
}
@media (max-width: 768px) {
	.tool{height:auto;}
	.tool li:nth-child(2n){width:49%}
	.tool li:nth-child(2n+1){width:49%}
