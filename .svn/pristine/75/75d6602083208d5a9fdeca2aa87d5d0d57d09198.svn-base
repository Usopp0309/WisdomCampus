@media (max-width: 768px) {
    .homepage .web_logo{display:none;}
	.homepage .homepage_header{height:auto}
	.homepage .homepage_header .logo{width:100%;}
	.homepage .homepage_header .logo span{font-size:20px;}
	.homepage .homepage_header .time{width:100%;text-align: left;text-indent: 15px;}
	.homepage .content .news_show{width:100%;}
	.homepage .content .word{width:100%;}
	.homepage .content .my_info_a_3{width:48%;}
	.homepage .content .school_news_count .school_news{height:auto}
	.homepage .content .public_info .info_header{min-height:315px;}
	.homepage .content .public_info{width:100%;margin:0 0 15px 0}
	.homepage .content .school_news_count{width:100%;}
	.homepage .system li .system_header img{display:block;margin:0 auto;}
	.contact{display:none;}
