#welcome .msg .con { padding: 0 30px 20px; }
#welcome .msg table { table-layout: fixed; border-top: 1px dotted #ccc; margin-bottom: 15px; width: 100%; }
#welcome .msg table.ctable0 { border-top: 0; }
#welcome .msg .pic { padding: 18px 5px 0 15px; width: 40px; }
#welcome .msg .ac { padding-top: 18px; }
#welcome .msg .auth { line-height: 18px; }
