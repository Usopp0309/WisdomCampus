#welcome table { width: 100%;}
#welcome table td { vertical-align: top; }
#welcome .right { width: 218px; padding-left: 25px; }

#welcome .title { text-align: center; font-size: 14px; font-weight: bold; padding: 15px 0; }

#welcome .board .con {padding: 0 15px 20px; line-height: 20px; }

.mes {background: url('menu/messageonline.png') no-repeat; width: 985px;height: 636px;}
