	<!-- 添加资产 -->
	<insert id="addAsset" parameterClass="asset">
		insert into palm_asset
		(
			code,
			name,
			type,
			property,
			manufacturer,
			respPersonId,
			respPerson,
			receiveId,
			spec,
			model,
			units,
			count,
			price,
			site,
			buyDate,
			qualityDate,
			totalPrice,
			remark,
			receiveTime,
			createTime
		)
		values
		(
			#code#,
			#name#,
			#type#,
			#property#,
			#manufacturer#,
			#respPersonId#,
			#respPerson#,
			#receiveId#,
			#spec#,
			#model#,
			#units#,
			#count#,
			#price#,
			#site#,
			#buyDate#,
			#qualityDate#,
			#totalPrice#,
			#remark#,
			#receiveTime#,
			#createTime#
		)
		<selectKey resultClass="int" type="post" keyProperty="id">
        	select LAST_INSERT_ID() as value
    	</selectKey>
	</insert>
	<!-- 修改资产 -->
	<update id="modifyAsset" parameterClass="asset">
		update palm_asset set 
			updateTime = #updateTime#
			<isNotEmpty property="code">
				,code = #code#
			</isNotEmpty>
			<isNotEmpty property="name">
				,name = #name#
			</isNotEmpty>
			<isNotEmpty property="type">
				,type = #type#
			</isNotEmpty>
			<isNotEmpty property="property">
				,property = #property#
			</isNotEmpty>
			<isNotEmpty property="manufacturer">
				,manufacturer = #manufacturer#
			</isNotEmpty>
			<isNotEmpty property="respPersonId">
				,respPersonId = #respPersonId#
			</isNotEmpty>
			<isNotEmpty property="respPerson">
				,respPerson = #respPerson#
			</isNotEmpty>
			<isNotEmpty property="spec">
				,spec = #spec#
			</isNotEmpty>
			<isNotEmpty property="model">
				,model = #model#
			</isNotEmpty>
			<isNotEmpty property="units">
				,units = #units#
			</isNotEmpty>
			<isNotEmpty property="count">
				,count = #count#
			</isNotEmpty>
			<isNotEmpty property="price">
				,price = #price#
			</isNotEmpty>
			<isNotEmpty property="site">
				,site = #site#
			</isNotEmpty>
			<isNotEmpty property="buyDate">
				,buyDate = #buyDate#
			</isNotEmpty>
			<isNotEmpty property="qualityDate">
				,qualityDate = #qualityDate#
			</isNotEmpty>
			<isNotEmpty property="totalPrice">
				,totalPrice = #totalPrice#
			</isNotEmpty>
			<isNotEmpty property="remark">
				,remark = #remark#
			</isNotEmpty>
			<isNotEmpty property="receiveId">
				,receiveId = #receiveId#
			</isNotEmpty>
			<isNotEmpty property="receiveTime">
				,receiveTime = #receiveTime#
			</isNotEmpty>
			where id = #id#
	</update>

	<!-- 添加维修人员 -->
	<insert id="addAssetServiceman" parameterClass="assetServiceman">
		insert into palm_asset_serviceman
		(
			servicemanId,
			serviceman,
			type,
			phone,
			code,
			leaderIdOne,
			leaderOne,
			leaderIdTwo,
			leaderTwo,
			createTime
		)
		values
		(
			#servicemanId#,
			#serviceman#,
			#type#,
			#phone#,
			#code#,
			#leaderIdOne#,
			#leaderOne#,
			#leaderIdTwo#,
			#leaderTwo#,
			#createTime#
		)
		<selectKey resultClass="int" type="post" keyProperty="id">
        	select LAST_INSERT_ID() as value
    	</selectKey>
	</insert>
	
	<!-- 根据ID获取详情 -->
	<select id="gerServicemanByServicemanIdAndType" parameterClass="HashMap" resultClass="assetServiceman">
		select * from palm_asset_serviceman where servicemanId = #servicemanId# and type = #type#
	</select>
	
