	<!-- 添加资产保修 -->
	<insert id="addAssetRepair" parameterClass="assetRepair">
		insert into palm_asset_repair
		(
			assetId,
			userId,
			realName,
			phone,
			type,
			servicemanId,
			price,
			repairPrice,
			status,
			remark,
			createTime,
			<isNotEmpty property="estimatedTime">
				estimatedTime,
			</isNotEmpty>
			<isNotEmpty property="receiveTime">
				receiveTime,
			</isNotEmpty>
			<isNotEmpty property="auditTime">
				auditTime,
			</isNotEmpty>
			updateTime
		)
		values
		(
			#assetId#,
			#userId#,
			#realName#,
			#phone#,
			#type#,
			#servicemanId#,
			#price#,
			#repairPrice#,
			#status#,
			#remark#,
			#createTime#,
			<isNotEmpty property="estimatedTime">
				#estimatedTime#,
			</isNotEmpty>
			<isNotEmpty property="receiveTime">
				#receiveTime#,
			</isNotEmpty>
			<isNotEmpty property="auditTime">
				#auditTime#,
			</isNotEmpty>
			#updateTime#
		)
		<selectKey resultClass="int" type="post" keyProperty="id">
        	select LAST_INSERT_ID() as value
    	</selectKey>
	</insert>
	
