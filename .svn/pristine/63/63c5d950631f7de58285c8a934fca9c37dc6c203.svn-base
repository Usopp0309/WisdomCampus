	<!-- 根据ID删除资产信息 -->
	<delete id="deleteAssetById" parameterClass="Integer">
		delete from palm_asset where id = #id#
	</delete>
	<!--删除维修人员 或者 安全隐患检查人员 -->
	<delete id="deleteAssetServicemanById" parameterClass="Integer">
