package com.guotop.palmschool.schoolnews.entity;

import com.google.gson.annotations.Expose;

/**
 * 校园新闻
 * 
 * @author
 */

public class SchoolNewsInfo
{
	/**
	 * 新闻Id
	 */
	@Expose
	private Integer newsId;

	/**
	 * 新闻类型：1、校园新闻，2、学校活动，3、学校招生信息  4、家长课堂
	 */
	@Expose
	private Integer newsType;

	/**
	 * 新闻标题
	 */
	@Expose
	private String newsTitle;

	/**
	 * 新闻作者
	 */
	private String newsAuthor;

	/**
	 * 新闻内容
	 */
	private String newsContent;

	/**
	 * 新闻封面
	 */
	@Expose
	private String newsCover;

	/**
	 * 新闻摘要
	 */
	@Expose
	private String newsDigest;

	/**
	 * 联系电话(招生时有咨询电话)
	 */
	private String phone;

	/**
	 * 新闻发布的范围T0表示全校教职工 P0全部家长 G开头表示年级 C开头表示班级
	 */
	private String newsRanye;
	
	/**
	 * 学校名字
	 */
	@Expose
	public String schoolName;

	/**
	 * 学校Id
	 */
	private String schoolId;

	/**
	 * 创建人Id
	 */
	private Integer createUserId;

	/**
	 * 创建人姓名
	 */
	private String createUserName;

	/**
	 * 创建时间
	 */
	private String createTime;
	
	/**
	 * 创建时间
	 */
	@Expose
	private long time;

	/**
	 * 默认0 表示没有电话和我要报名功能 1只有我要咨询功能 2 只有我要报名功能 3两者都有
	 */
	private Integer functionType;

	/**
	 * 是否下线，0、下线，1、上线
	 */
	private Integer status;

	/**
	 * 新闻链接
	 */
	@Expose
	private String newsSrc;
	
	/**
	 * 参加数量  针对活动  
	 */
	@Expose
	private Integer number;

	public Integer getFunctionType()
	{
		return functionType;
	}

	public void setFunctionType(Integer functionType)
	{
		this.functionType = functionType;
	}

	public Integer getStatus()
	{
		return status;
	}

	public void setStatus(Integer status)
	{
		this.status = status;
	}

	public Integer getNewsId()
	{
		return newsId;
	}

	public void setNewsId(Integer newsId)
	{
		this.newsId = newsId;
	}

	public Integer getNewsType()
	{
		return newsType;
	}

	public void setNewsType(Integer newsType)
	{
		this.newsType = newsType;
	}

	public String getNewsTitle()
	{
		return newsTitle;
	}

	public void setNewsTitle(String newsTitle)
	{
		this.newsTitle = newsTitle;
	}

	public String getNewsAuthor()
	{
		return newsAuthor;
	}

	public void setNewsAuthor(String newsAuthor)
	{
		this.newsAuthor = newsAuthor;
	}

	public String getNewsContent()
	{
		return newsContent;
	}

	public void setNewsContent(String newsContent)
	{
		this.newsContent = newsContent;
	}

	public String getNewsCover()
	{
		return newsCover;
	}

	public void setNewsCover(String newsCover)
	{
		this.newsCover = newsCover;
	}

	public String getNewsDigest()
	{
		return newsDigest;
	}

	public void setNewsDigest(String newsDigest)
	{
		this.newsDigest = newsDigest;
	}

	public String getPhone()
	{
		return phone;
	}

	public void setPhone(String phone)
	{
		this.phone = phone;
	}

	public String getNewsRanye()
	{
		return newsRanye;
	}

	public void setNewsRanye(String newsRanye)
	{
		this.newsRanye = newsRanye;
	}

	public String getSchoolId()
	{
		return schoolId;
	}

	public void setSchoolId(String schoolId)
	{
		this.schoolId = schoolId;
	}

	public Integer getCreateUserId()
	{
		return createUserId;
	}

	public void setCreateUserId(Integer createUserId)
	{
		this.createUserId = createUserId;
	}

	public String getCreateUserName()
	{
		return createUserName;
	}

	public void setCreateUserName(String createUserName)
	{
		this.createUserName = createUserName;
	}

	public String getCreateTime()
	{
		return createTime;
	}

	public void setCreateTime(String createTime)
	{
		this.createTime = createTime;
	}

	public String getNewsSrc()
	{
		return newsSrc;
	}

	public void setNewsSrc(String newsSrc)
	{
		this.newsSrc = newsSrc;
	}

	public String getSchoolName()
	{
		return schoolName;
	}

	public void setSchoolName(String schoolName)
	{
		this.schoolName = schoolName;
	}

	public long getTime()
	{
		return time;
	}

	public void setTime(long time)
	{
		this.time = time;
	}

	public Integer getNumber()
	{
		return number;
	}

	public void setNumber(Integer number)
	{
		this.number = number;
	}

	
}
