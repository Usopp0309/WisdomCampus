package com.guotop.palmschool.entity;

import java.io.Serializable;
/**
 * 学校管理页面展示内容实体类
 * @author jfy
 * @date 2015年12月8日
 */
public class SchoolInfo implements Serializable
{
	/**
	 * 
	 */
	private static final long serialVersionUID = 8625542039117846682L;
	//学校ID
	private Long schoolId;
	//学校名称
	private String schoolName;
	//学校地址
	private String schoolAddress;
	//校长姓名
	private String president;
	//校徽图路径
	private String schoolBadge;
	//用户数量 
	private Integer userCount;
	//学生数量
	private Integer studentCount;
	//家长数量
	private Integer parentCount;
	//教师数量
	private Integer teacherCount;
	//短信数量
	private Integer smsCount;
	//移动短信总数
	private Integer mobileSmsCount;
	//联通短信总数
	private Integer unicomSmsCount;
	//电信短信总数
	private Integer telecomSmsCount;
	//管理员登陆账户
	private String adminUserName;
	//管理员姓名
	private String adminRealName;

	public Long getSchoolId()
	{
		return schoolId;
	}

	public void setSchoolId(Long schoolId)
	{
		this.schoolId = schoolId;
	}

	public String getSchoolName()
	{
		return schoolName;
	}

	public void setSchoolName(String schoolName)
	{
		this.schoolName = schoolName;
	}

	public String getSchoolAddress()
	{
		return schoolAddress;
	}

	public void setSchoolAddress(String schoolAddress)
	{
		this.schoolAddress = schoolAddress;
	}

	public String getPresident()
	{
		return president;
	}

	public void setPresident(String president)
	{
		this.president = president;
	}

	public String getSchoolBadge()
	{
		return schoolBadge;
	}

	public void setSchoolBadge(String schoolBadge)
	{
		this.schoolBadge = schoolBadge;
	}

	public Integer getUserCount()
	{
		return userCount;
	}

	public void setUserCount(Integer userCount)
	{
		this.userCount = userCount;
	}

	public Integer getStudentCount()
	{
		return studentCount;
	}

	public void setStudentCount(Integer studentCount)
	{
		this.studentCount = studentCount;
	}

	public Integer getTeacherCount()
	{
		return teacherCount;
	}

	public void setTeacherCount(Integer teacherCount)
	{
		this.teacherCount = teacherCount;
	}

	public Integer getSmsCount()
	{
		return smsCount;
	}

	public void setSmsCount(Integer smsCount)
	{
		this.smsCount = smsCount;
	}

	public String getAdminUserName()
	{
		return adminUserName;
	}

	public void setAdminUserName(String adminUserName)
	{
		this.adminUserName = adminUserName;
	}

	public Integer getMobileSmsCount()
	{
		return mobileSmsCount;
	}

	public void setMobileSmsCount(Integer mobileSmsCount)
	{
		this.mobileSmsCount = mobileSmsCount;
	}

	public Integer getUnicomSmsCount()
	{
		return unicomSmsCount;
	}

	public void setUnicomSmsCount(Integer unicomSmsCount)
	{
		this.unicomSmsCount = unicomSmsCount;
	}

	public Integer getTelecomSmsCount()
	{
		return telecomSmsCount;
	}

	public void setTelecomSmsCount(Integer telecomSmsCount)
	{
		this.telecomSmsCount = telecomSmsCount;
	}

	public String getAdminRealName()
	{
		return adminRealName;
	}

	public void setAdminRealName(String adminRealName)
	{
		this.adminRealName = adminRealName;
	}

	public Integer getParentCount()
	{
		return parentCount;
	}

	public void setParentCount(Integer parentCount)
	{
		this.parentCount = parentCount;
	}
	
	
}
