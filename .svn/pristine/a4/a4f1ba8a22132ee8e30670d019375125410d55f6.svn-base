package com.guotop.palmschool.scheduler.entity;

import java.io.Serializable;

/**
 * 学校统计信息实体类
 * @author jfy
 * @date 2015年12月7日
 */
public class SchoolCountInfo implements Serializable
{
	/**
	 * 
	 */
	private static final long serialVersionUID = 2829384337817256855L;
	// 学校ID
	private Long schoolId;
	//用户总数
	private Integer userCount;
	//老师总数
	private Integer teacherCount;
	//学生总数
	private Integer studentCount;
	//短信总数
	private Integer smsCount;
	//移动短信总数 
	private Integer mobileSmsCount;
	//联通短信总数
	private Integer unicomSmsCount;
	//电信短信总数
	private Integer telecomSmsCount;
	
	/*
	 * 自定义属性
	 */
	//代理商名称
	private String agencyName;
	//代理商名称
	private Integer agencyId;
	//学校名称
	private String schoolName;
	//学校服务费缴费总额
	private Double serviceTotalAmount;
	//智慧校园卡总数
	private Integer cardNumber;
	//智慧校园卡押金总额
	private Double cardTotalDepositAmount;
	
	public Long getSchoolId()
	{
		return schoolId;
	}
	public void setSchoolId(Long schoolId)
	{
		this.schoolId = schoolId;
	}
	public Integer getUserCount()
	{
		return userCount;
	}
	public void setUserCount(Integer userCount)
	{
		this.userCount = userCount;
	}
	public Integer getTeacherCount()
	{
		return teacherCount;
	}
	public void setTeacherCount(Integer teacherCount)
	{
		this.teacherCount = teacherCount;
	}
	public Integer getStudentCount()
	{
		return studentCount;
	}
	public void setStudentCount(Integer studentCount)
	{
		this.studentCount = studentCount;
	}
	public Integer getSmsCount()
	{
		return smsCount;
	}
	public void setSmsCount(Integer smsCount)
	{
		this.smsCount = smsCount;
	}
	public Integer getMobileSmsCount()
	{
		return mobileSmsCount;
	}
	public void setMobileSmsCount(Integer mobileSmsCount)
	{
		this.mobileSmsCount = mobileSmsCount;
	}
	public Integer getUnicomSmsCount()
	{
		return unicomSmsCount;
	}
	public void setUnicomSmsCount(Integer unicomSmsCount)
	{
		this.unicomSmsCount = unicomSmsCount;
	}
	public Integer getTelecomSmsCount()
	{
		return telecomSmsCount;
	}
	public void setTelecomSmsCount(Integer telecomSmsCount)
	{
		this.telecomSmsCount = telecomSmsCount;
	}
	public String getAgencyName()
	{
		return agencyName;
	}
	public void setAgencyName(String agencyName)
	{
		this.agencyName = agencyName;
	}
	public Integer getAgencyId()
	{
		return agencyId;
	}
	public void setAgencyId(Integer agencyId)
	{
		this.agencyId = agencyId;
	}
	public String getSchoolName()
	{
		return schoolName;
	}
	public void setSchoolName(String schoolName)
	{
		this.schoolName = schoolName;
	}
	public Double getServiceTotalAmount()
	{
		return serviceTotalAmount;
	}
	public void setServiceTotalAmount(Double serviceTotalAmount)
	{
		this.serviceTotalAmount = serviceTotalAmount;
	}
	public Integer getCardNumber()
	{
		return cardNumber;
	}
	public void setCardNumber(Integer cardNumber)
	{
		this.cardNumber = cardNumber;
	}
	public Double getCardTotalDepositAmount()
	{
		return cardTotalDepositAmount;
	}
	public void setCardTotalDepositAmount(Double cardTotalDepositAmount)
	{
		this.cardTotalDepositAmount = cardTotalDepositAmount;
	}
	
}
