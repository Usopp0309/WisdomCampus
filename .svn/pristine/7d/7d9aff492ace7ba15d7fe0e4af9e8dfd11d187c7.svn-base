package com.guotop.palmschool.entity;

public class UserForwardsmsSwitch
{
	/**
	 * 主键
	 */
	private int id;

	/**
	 * 推送转发状态:0停用，1启用
	 */
	private int forwardPushStatus;

	/**
	 * 短信转发状态:0停用，1启用
	 */
	private int forwardSmsStatus;

	private int userId;

	public int getId()
	{
		return this.id;
	}

	public void setId(int id)
	{
		this.id = id;
	}

	public int getForwardPushStatus()
	{
		return this.forwardPushStatus;
	}

	public void setForwardPushStatus(int forwardPushStatus)
	{
		this.forwardPushStatus = forwardPushStatus;
	}

	public int getForwardSmsStatus()
	{
		return this.forwardSmsStatus;
	}

	public void setForwardSmsStatus(int forwardSmsStatus)
	{
		this.forwardSmsStatus = forwardSmsStatus;
	}

	public int getUserId()
	{
		return this.userId;
	}

	public void setUserId(int userId)
	{
		this.userId = userId;
	}

}