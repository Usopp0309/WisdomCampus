package com.guotop.palmschool.recipe.entity;import java.io.Serializable;import java.util.ArrayList;import java.util.List;import com.guotop.palmschool.entity.Dish;public class Recipe implements Serializable{	private static final long serialVersionUID = 1L;	private int id;	private String createTime;	private String pushDate;	private String week;	private List<Dish> dishList = new ArrayList<Dish>();	private List<Dish> breakfastList = new ArrayList<Dish>();	private List<Dish> lunchList = new ArrayList<Dish>();	private List<Dish> snackList = new ArrayList<Dish>();	private List<Dish> otherList = new ArrayList<Dish>();	public int getId()	{		return id;	}	public void setId(int id)	{		this.id = id;	}	public String getCreateTime()	{		return createTime;	}	public void setCreateTime(String createTime)	{		this.createTime = createTime;	}	public String getPushDate()	{		return pushDate;	}	public void setPushDate(String pushDate)	{		this.pushDate = pushDate;	}	public List<Dish> getDishList()	{		return dishList;	}	public void setDishList(List<Dish> dishList)	{		this.dishList = dishList;	}	public List<Dish> getBreakfastList()	{		return breakfastList;	}	public void setBreakfastList(List<Dish> breakfastList)	{		this.breakfastList = breakfastList;	}	public List<Dish> getLunchList()	{		return lunchList;	}	public void setLunchList(List<Dish> lunchList)	{		this.lunchList = lunchList;	}	public List<Dish> getSnackList()	{		return snackList;	}	public void setSnackList(List<Dish> snackList)	{		this.snackList = snackList;	}	public List<Dish> getOtherList()	{		return otherList;	}	public void setOtherList(List<Dish> otherList)	{		this.otherList = otherList;	}	public String getWeek()	{		return week;	}	public void setWeek(String week)	{		this.week = week;	}}