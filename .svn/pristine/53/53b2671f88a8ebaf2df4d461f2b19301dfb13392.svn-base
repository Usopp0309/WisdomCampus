	@Override
	public boolean equals(Object obj)
	{
		if (this == obj)
			return false;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		User user = (User) obj;

		if (userId.equals(user.userId))
		{
			if (user.parentId != null && parentId.equals(user.parentId))
			{
				return true;
			} else if (user.parentId == null)
			{
				return true;
			}
		}
		return false;
	}

