.footer .footer-inner {text-align: center;width: 100%;}
.NormalCss{background:url('../image/sort_both.png') no-repeat center right;background-repeat:no-repeat;background-position:right center;}
.SortDescCss{background:url('../image/sort_desc.png') no-repeat center right;background-repeat:no-repeat;background-position:right center;}
.SortAscCss{background:url('../image/sort_asc.png') no-repeat center right;background-repeat:no-repeat;background-position:right center;}
