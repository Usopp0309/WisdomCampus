.public_page{width:100%;margin:15px auto 0px;text-align: center;}
.public_page li{margin:0px auto;display:inline;margin:0px 15px;}

.Permission,.personnels{display:none;float:left;margin:0 15px 0 0;width:250px;border:1px solid #cdcdcd;}
.Permission p,.personnels p{background-color:#efefef;color:#676767;z-index:8; margin:0px;top:1px;left:1px;width:229px;padding:5px 10px;text-indent: 30px;position: relative;}
.Permission p .checkbox,.personnels p .checkbox{position: absolute; top:5px;left:10px;width:17px;min-height:17px!important;margin:0px;background-color:#fff!important;text-indent:0px;text-align:center; line-height:20px;border:1px solid #cccccc;color:#2d7ebd;}
.Permission p .search_m,.personnels p .search_m{position: absolute; top:0px;right:20px;cursor: pointer;margin:7px 0 0 0}
.Permission p i,.personnels p i{position: absolute; top:0px;right:20px;cursor: pointer;margin:7px 0 0 0}
.Permission p .search_box,.personnels p .search_box{position: absolute; left:0px;bottom:-40px;display:none;background-color:#fff;z-index:9;border-bottom:1px solid #cdcdcd;margin:0px;height:40px;}
.Permission p .search_box .input_text,.personnels p .search_box .input_text{margin:0px;float:left;padding:0 5px;position: static;width:130px;height:25px;line-height:25px;margin:5px 0 0 20px;}
.Permission p .search_box .input_button,.personnels p .search_box .input_button{margin:0px;float:left;padding:0 10px; position: static;height:27px;margin:5px 0 0 5px;border:1px solid #cdcdcd;color:#666;}
.Permission dl,.personnels dl{height:300px;width:250px;margin:0px;overflow-x:hidden;overflow-y:block;position:relative;}
.Permission dl dd,.personnels dl dd{padding:5px 10px;text-indent: 30px;margin:0px;position: relative;cursor: pointer;}
.Permission dl dd i,.personnels dl dd i{position: absolute;background-color:#fff!important; top:5px;left:10px;width:17px;height:17px;min-height:17px!important;border:1px solid #cccccc;color:#2d7ebd;text-indent: 0px;text-align: center;}
.Permission dl .checked,.personnels dl .checked{background-color: #32c5d2 ;color: #fff;}
.Permission dl dd .next_menu{color: #4eb2f6;height: 17px;right: 10px;line-height: 20px;position: absolute;text-align: center;text-indent: 0;top: 5px;width: 17px;}

.settime p{position: relative;padding:10px 0px;text-indent: 30px;}
.settime p .checkbox{border:1px solid #ccc;background-color:#fff;width:20px;line-height:20px;min-height:20px; text-align:center; text-indent:0px;color:#ccc; margin:0px 10px 0 0;cursor:pointer; display:inline-block;position: absolute;top:10px;left:5px;}
.time_con{display:none;}
.selectObject{height:300px;border:1px solid #c2cad8;margin:0 2% 0 0;}
.selectObject p,.selectObject ul li{height:40px;line-height:40px;border-bottom:1px solid #c2cad8;position: relative;text-indent: 35px;margin:0px;padding:0px;}
.selectObject p i,.selectObject ul li i{padding:8px 0;border:1px solid #999;text-indent:0px; position:absolute;width:15px;height:15px;padding:0px;top:8px;left:8px;line-height:15px!important;text-align: center;color:#999;cursor: pointer;}
.selectObject ul{width:100%;}
.selectObject p .fa-check,.selectObject ul li .fa-check{text-indent:0px;}
.td_row,.schedule_detail{display:block!important;margin:0px auto;width:15px;height:15px;border:1px solid #999;padding:0px;line-height:15px!important;text-align: center;color:#999;cursor: pointer;}
.schedule_time p span{margin:0 10px 0 0;display:inline-block;text-indent: 30px;position: relative;}
.schedule_time p span i{padding:8px 0;border:1px solid #999;text-indent:0px;display:inline-block; width:15px;height:15px;padding:0px;position:absolute;top:0px;left:0px; line-height:15px!important;text-align: center;color:#999;cursor: pointer;}


.replaceMonth,.replaceNumber,.setMedal{border:1px solid #eaeaea;height:160px;margin-top:20px;position: relative;}
.replaceMonth h2,.replaceNumber h2,.setMedal h2{margin:0px;text-align:left;padding:0px 15px;height:40px;line-height:40px;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;font-size:14px;font-weight:bold}
.replaceMonth .info,.replaceNumber .info,.setMedal .info{height:120px;width:75%;}
.replaceMonth .info b,.replaceNumber .info b,.setMedal .info b{width:26%;height:76px;margin:5px 4%;background-color:#27aae2;color:#fff;display:block;float:left;text-align: center;font-size:40px;}
.replaceMonth .info b i,.replaceNumber .info b i,.setMedal .info b i{line-height:76px;}
.replaceMonth .info p,.replaceNumber .info p,.setMedal .info p{width:60%;height:120px;float:left;display:block;margin:0px;padding:0px;}
.replaceMonth .info p span,.replaceNumber .info p span,.setMedal .info p span{height:25px;line-height:25px;display:block;text-align: left;overflow:hidden;text-overflow: ellipsis;white-space: nowrap;}
.replaceMonth .info p span em,.replaceNumber .info p span em,.setMedal .info p span em{font-style:normal;font-weight:bold;}
.replaceMonth .info p span input,.replaceNumber .info p span input,.setMedal .info p span input{width:50px;display:inline-block;height:23px;line-height:23px;padding:0px;margin:0px 10px 2px;border:1px solid #ccc;background-color:#fff;}
.replaceMonth .m_btn,.replaceNumber .m_btn,.setMedal .m_btn{width:20%;position: absolute;right:5px;bottom:5px;}
.schedule_person_p {font-size: 24px;font-weight: bold;padding: 25px 0;text-align: center;}
.zheng, .shangke {background-color: #34aa49;border-radius: 3px; color: #fff;font-style: normal;font-weight: bold;margin: 0 10px 0 0;padding: 5px;}
